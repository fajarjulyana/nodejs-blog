<div class="author">
    <h1 class="author__heading">Hi, I am Fajar.</h1>
    <p class="author__body">Web Developer and content creator.</p>
</div>

<img src="/img/hero.jpg" alt="person looking screen" class="hero-image" width="981" height="528">

<section class="article">
    <h2 class="article__heading">Lastest Posts</h2>

    <ul class="article-ul">
        <% data.forEach(post => { %>
        <li>
            <a href="#">
                <span><%= post.title %></span>
                <span class="article-list__date"><%= post.createdAt %></span>
            </a>
        </li>
        <% }) %>
    </ul>

    <a href="#" class="pagination">&lt; View Older Posts</a>
</section>